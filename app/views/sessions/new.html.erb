<h1>Sign in</h1>

<%= form_with url: sign_in_path, scope: :session do |f| %>
  <div>
    <%= f.label :email %><br>
    <%= f.email_field :email, required: true, autofocus: true %>
  </div>

  <div>
    <%= f.label :password %><br>
    <%= f.password_field :password, required: true %>
  </div>

  <div>
    <%= f.submit "Sign in" %>
  </div>
<% end %>
